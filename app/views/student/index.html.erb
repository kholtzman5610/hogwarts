<h1>Current Students</h1>

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>House</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  
  <tbody>
      <% @students.each do |student| %>
      <tr>
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.house.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>