<h1>Current Professors</h1>

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>House</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  
  <tbody>
      <% @professors.each do |professor| %>
      <tr>
        <td><%= professor.first_name %></td>
        <td><%= professor.last_name %></td>
        <td><%= professor.house.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>